  "overrides": {
    "react-scripts": {
      "typescript": "^5.5.4"
    },
    "stylelint-config-prettier": {
      "stylelint": "^16.9.0"
    }
  },
  "scripts": {
    "update-browserslist": "npx update-browserslist-db@latest",
    "build-sitemap": "tsc sitemap-generator.ts && node sitemap-generator.js",
    "build-robots.txt": "tsc robots-generator.ts && node robots-generator.js",
    "build-extras": "npm run build-sitemap && npm run build-robots.txt",
    "depcheck": "node depcheck.ts",
    "start": "react-scripts start",
    "dev": "WATCHPACK_POLLING=true react-scripts start",
    "build": "react-scripts build",
    "serve-build": "serve -s build",
    "snap": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "reactSnap": {
    "inlineCss": true,
    "removeBlobs": true,
    "skipThirdPartyRequests": true,
    "minifyHtml": {
      "collapseWhitespace": true
    },
    "crawl": false,
    "include": [
      "/",
      "/privacy-policy",
      "/terms-of-service",
      "/signin",
      "/signup",
      "/dist"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
